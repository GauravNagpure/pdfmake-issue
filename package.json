{
  "name": "renewables",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "hmr": "ng serve --configuration=hmr --open",
    "gzipper": "gzipper",
    "build": "ng build --outputHashing=all && gzipper --verbose ./dist",
    "build:prod": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --outputHashing=all && gzipper --verbose ./dist",
    "build:qa": "ng build --configuration=qa --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --outputHashing=all && gzipper --verbose ./dist ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.ts\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts}": [
      "tslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@angular/animations": "8.1.1",
    "@angular/common": "8.1.1",
    "@angular/compiler": "8.1.1",
    "@angular/core": "8.1.1",
    "@angular/forms": "8.1.1",
    "@angular/platform-browser": "8.1.1",
    "@angular/platform-browser-dynamic": "8.1.1",
    "@angular/router": "8.1.1",
    "@ant-design/icons-angular": "9.0.1",
    "@elastic/apm-rum-angular": "1.1.9",
    "@ng-bootstrap/ng-bootstrap": "5.1.0",
    "@ngrx/router-store": "8.1.0",
    "@ngrx/store": "8.1.0",
    "@ngx-progressbar/core": "5.3.2",
    "@ngx-progressbar/http": "5.3.2",
    "@ngx-progressbar/router": "5.3.2",
    "bootstrap-css-only": "4.4.1",
    "core-js": "3.8.2",
    "crypto-js": "4.0.0",
    "export-to-csv": "0.2.1",
    "file-saver": "2.0.2",
    "fuzzy-search": "3.2.1",
    "hammerjs": "2.0.8",
    "jspdf": "2.5.1",
    "lodash": "4.17.14",
    "logrocket": "1.0.14",
    "moment": "2.26.0",
    "ng-chartist": "4.1.0",
    "ng-zorro-antd": "8.1.2",
    "ng2-nvd3": "2.0.0",
    "ngx-daterangepicker-material": "1.3.4",
    "ngx-nestable": "0.9.1",
    "ngx-perfect-scrollbar": "8.0.0",
    "ngx-permissions": "8.1.1",
    "ngx-scrollbar": "4.2.0",
    "ngx-sortablejs": "3.1.3",
    "nvd3": "1.8.6",
    "pdfmake": "0.2.5",
    "qs": "6.7.0",
    "rxjs": "6.5.3",
    "sortablejs": "1.10.0-rc3",
    "store": "2.0.12",
    "tslib": "2.3.1",
    "xlsx": "0.16.4",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.1",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "8.1.1",
    "@angular/language-service": "8.1.1",
    "@angularclass/hmr": "2.1.3",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.6.2",
    "codelyzer": "5.1.0",
    "gzipper": "3.7.0",
    "husky": "3.0.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "protractor": "5.4.2",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-standard": "18.3.0",
    "ts-node": "8.3.0",
